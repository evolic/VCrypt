{
    "name" : "loculus/vcrypt",
    "description" : "Vigenère cipher for PHP",
    "keywords" : [
        "Vigenère cipher",
        "Vigenère square",
        "Vigenère table",
        "the tabula recta"
    ],
    "homepage" : "https://github.com/loculus/vcrypt",
    "license" : [
        "BSD-3-Clause"
    ],
    "repositories": [{
        "type": "package",
        "package": {
            "name": "sebastianbergmann/phpcov",
            "version": "1.1.0",
            "dist": {
                "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                "type": "zip"
            },
            "source": {
                "url": "https://github.com/sebastianbergmann/phpcov.git",
                "type": "git",
                "reference": "1.1.0"
            },
            "bin": [
                "phpcov.php"
            ]
        }
    }],
    "require" : {
        "php" : ">=5.3.23",
        "zendframework/zendframework" : ">=2.3"
    },
    "require-dev" : {
        "sebastianbergmann/phpcov": "1.1.0",
        "fabpot/php-cs-fixer": "dev-master",
        "squizlabs/php_codesniffer": "~1.5.*",
        "phpmd/phpmd": "~2.1.*",
        "sebastian/phpcpd": "~2.0.*",
        "pdepend/pdepend": "~2.0.*",
        "phploc/phploc": "~2.0.*",
        "phpunit/phpunit": "~4.3.*",
        "theseer/phpdox": "~0.7.*"
    },
    "autoload": {
        "psr-0": {
            "VCrypt\\": "library/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev",
            "dev-develop": "0.4-dev"
        }
    }
}
